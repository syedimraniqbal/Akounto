                        <com.skydoves.powerspinner.PowerSpinnerView
                            android:id="@+id/currencySpinner"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@drawable/general"
                            android:fontFamily="@font/poppins_regular"
                            android:hint="Please select currency"
                            android:padding="10dp"
                            android:textColor="@color/blackTextColor"
                            android:textColorHint="@color/blackTextColor"
                            android:textSize="@dimen/_12sdp"
                            app:spinner_arrow_gravity="end"
                            app:spinner_arrow_padding="8dp"
                            app:spinner_arrow_tint="@color/teal_700"
                            app:spinner_divider_color="@color/dark_grey"
                            app:spinner_divider_show="true"
                            app:spinner_divider_size="0.4dp"
                            app:spinner_item_array="@array/merchant"
                            app:spinner_popup_animation="normal"
                            app:spinner_popup_background="@color/white"
                            app:spinner_popup_elevation="14dp" />
                    </RelativeLayout>
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/relativeLayoutNameBDCountry"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp">

                    <TextView
                        android:id="@+id/textViewNameBDCountry"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/poppins_regular"
                        android:text="Country"
                        android:textColor="#333333"
                        android:textSize="@dimen/_14sdp" />

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
