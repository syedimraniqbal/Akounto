                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/relativeLayoutDescriptionBDDiff"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp">

                    <TextView
                        android:id="@+id/textViewDescriptionBDDiff"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/poppins_regular"
                        android:text="Address Line 2"
                        android:textColor="#333333"
                        android:textSize="@dimen/_14sdp" />

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/textViewDescriptionBDDiff"
                        android:layout_marginRight="@dimen/_10sdp"
                        >

                        <EditText
                            android:id="@+id/et_shipAddr2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@drawable/general"
                            android:padding="@dimen/_8sdp"
                            android:textColor="@color/text_color" />
                    </RelativeLayout>
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/relativeLayoutPriceBDDiff"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp">

                    <TextView
                        android:id="@+id/textViewPriceBDDiff"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
