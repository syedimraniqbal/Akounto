                        android:layout_height="wrap_content"
                        android:fontFamily="@font/poppins_regular"
                        android:text="Country"
                        android:textColor="#333333"
                        android:textSize="@dimen/_14sdp" />

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
