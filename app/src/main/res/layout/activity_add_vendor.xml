                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@drawable/general"
                            android:padding="@dimen/_8sdp"
                            android:textColor="@color/text_color" />
                    </RelativeLayout>
                </RelativeLayout>

                <RelativeLayout
